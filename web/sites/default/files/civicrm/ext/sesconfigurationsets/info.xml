<?xml version="1.0"?>
<extension key="sesconfigurationsets" type="module">
  <file>sesconfigurationsets</file>
  <name>SES Configuration Set</name>
  <description>Adds in the ability to set a Configuration Set header for use in Amazon SES tracking</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Brighten Zhang</author>
    <email>brighten.zhang@jmaconsulting.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.jmaconsulting.biz/extensions/ses-configuration-sets/sesconfigurationsets</url>
    <url desc="Documentation">https://lab.jmaconsulting.biz/extensions/ses-configuration-sets/sesconfigurationsets</url>
    <url desc="Support">https://lab.jmaconsulting.biz/extensions/ses-configuration-sets/sesconfigurationsets/-/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-10-10</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.74</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Sesconfigurationsets</namespace>
    <format>25.01.1</format>
    <angularModule>crmSesconfigurationsets</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
</extension>
