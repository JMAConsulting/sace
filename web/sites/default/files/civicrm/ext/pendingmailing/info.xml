    <url desc="Documentation">https://lab.civicrm.org/extensions/pendingmailing/tree/master</url>
    <url desc="Support">https://lab.civicrm.org/extensions/pendingmailing/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-05-30</releaseDate>
  <version>1.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.49</ver>
  </compatibility>
  <comments/>
