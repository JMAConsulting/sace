<?xml version="1.0"?>
<extension key="activity_type_program" type="module">
  <file>activity_type_program</file>
  <name>Activity Type Program</name>
  <description>Allows categorization of activity types by Drupal User Team</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Lauren Smillie</author>
    <email>lauren.smillie@jmaconsulting.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
