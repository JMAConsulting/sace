<?xml version="1.0"?>
<extension key="locked_notes" type="module">
  <file>locked_notes</file>
  <name>Locked Notes</name>
  <description>Add locking functionality to notes</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>LaurenSmillie</author>
    <email>lauren.smillie@jmaconsulting.biz</email>
  </maintainer>
  <releaseDate>2024-07-31</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.73</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/LockedNotes</namespace>
    <format>23.02.1</format>
    <angularModule>crmLockedNotes</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_LockedNotes_Upgrader</upgrader>
</extension>
