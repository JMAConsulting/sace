diff --git a/web/sites/default/files/civicrm/ext/userteamactivity/CRM/Userteamactivity/Upgrader/Base.php b/web/sites/default/files/civicrm/ext/userteamactivity/CRM/Userteamactivity/Upgrader/Base.php
