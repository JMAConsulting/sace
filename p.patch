+        'title' => $taxonomyTerm->label() . '  - User Team',
+        'menu_name' => 'main',
+        'link' => ['uri' => 'internal:/calendar-team/' . $taxonomyTerm->id()],
+        'parent' => $menu_link_parent->getPluginId(),
+      ])->save();
+    }
+  }
