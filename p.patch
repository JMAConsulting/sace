+function sace_custom_update_9001() {
+  $menu_link_parents = \Drupal::entityTypeManager()->getStorage('menu_link_content')
+    ->loadByProperties([
+      'title' => 'Calendars',
+      'menu_name' => 'main',
+    ]);
